<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ScrollView
        android:layout_width="409dp"
        android:layout_height="729dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <TableLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="414sp"
                    android:layout_height="wrap_content"
                    android:text="Peripherals"
                    android:textAlignment="center"
                    android:textColor="#1E91DD"
                    android:textSize="60sp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TableLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TableRow
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <ImageView
                            android:id="@+id/imageView21"
                            android:layout_width="170sp"
                            android:layout_height="170sp"
                            android:layout_marginStart="20sp"
                            android:layout_marginLeft="20sp"
                            android:layout_marginTop="10sp"
                            android:layout_marginRight="10sp"
                            android:layout_marginBottom="10sp"
                            app:srcCompat="@drawable/duckyone3" />

                        <ImageView
                            android:id="@+id/imageView22"
                            android:layout_width="170sp"
                            android:layout_height="170sp"
                            android:layout_marginStart="30sp"
                            android:layout_marginLeft="20sp"
                            android:layout_marginTop="10sp"
                            android:layout_marginRight="10sp"
                            android:layout_marginBottom="0sp"
                            app:srcCompat="@drawable/logitechg502" />
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <ImageView
                            android:id="@+id/imageView23"
                            android:layout_width="170sp"
                            android:layout_height="170sp"
                            android:layout_marginStart="20sp"
                            android:layout_marginTop="10sp"
                            android:layout_marginBottom="10sp"
                            app:srcCompat="@drawable/logitechg935" />

                        <ImageView
                            android:id="@+id/imageView24"
                            android:layout_width="170sp"
                            android:layout_height="170sp"
                            android:layout_marginStart="30sp"
                            android:layout_marginTop="10sp"
                            android:layout_marginRight="10sp"
                            app:srcCompat="@drawable/microphone" />
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" >

                        <ImageView
                            android:id="@+id/imageView25"
                            android:layout_width="10sp"
                            android:layout_height="170sp"
                            android:layout_marginStart="20sp"
                            android:layout_marginTop="10sp"
                            android:layout_marginRight="10sp"
                            android:layout_marginBottom="10sp"
                            app:srcCompat="@drawable/speaker" />

                        <ImageView
                            android:id="@+id/imageView26"
                            android:layout_width="170sp"
                            android:layout_height="170sp"
                            android:layout_marginStart="30sp"
                            android:layout_marginTop="10sp"
                            android:layout_marginRight="10sp"
                            android:layout_marginBottom="10sp"
                            app:srcCompat="@drawable/controller" />
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" >

                        <ImageView
                            android:id="@+id/imageView27"
                            android:layout_width="170sp"
                            android:layout_height="170sp"
                            android:layout_marginStart="20sp"
                            android:layout_marginTop="10sp"
                            android:layout_marginRight="10sp"
                            android:layout_marginBottom="10sp"
                            app:srcCompat="@drawable/monitor" />

                        <ImageView
                            android:id="@+id/imageView28"
                            android:layout_width="170sp"
                            android:layout_height="170sp"
                            android:layout_marginStart="30sp"
                            android:layout_marginRight="10sp"
                            android:layout_marginBottom="10sp"
                            app:srcCompat="@drawable/mousepad" />
                    </TableRow>
                </TableLayout>
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </TableLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>